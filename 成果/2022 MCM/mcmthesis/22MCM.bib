
@article{sreedhara_survey_2019,
	title = {A survey of mathematical models of human performance using power and energy},
	volume = {5},
	issn = {2198-9761},
	url = {https://doi.org/10.1186/s40798-019-0230-z},
	doi = {10.1186/s40798-019-0230-z},
	abstract = {The ability to predict the systematic decrease of power during physical exertion gives valuable insights into health, performance, and injury. This review surveys the research of power-based models of fatigue and recovery within the area of human performance. Upon a thorough review of available literature, it is observed that the two-parameter critical power model is most popular due to its simplicity. This two-parameter model is a hyperbolic relationship between power and time with critical power as the power-asymptote and the curvature constant denoted by W′. Critical power (CP) is a theoretical power output that can be sustained indefinitely by an individual, and the curvature constant (W′) represents the amount of work that can be done above CP. Different methods and models have been validated to determine CP and W′, most of which are algebraic manipulations of the two-parameter model. The models yield different CP and W′ estimates for the same data depending on the regression fit and rounding off approximations. These estimates, at the subject level, have an inherent day-to-day variability called intra-individual variability (IIV) associated with them, which is not captured by any of the existing methods. This calls for a need for new methods to arrive at the IIV associated with CP and W′. Furthermore, existing models focus on the expenditure of W′ for efforts above CP and do not model its recovery in the sub-CP domain. Thus, there is a need for methods and models that account for (i) the IIV to measure the effectiveness of individual training prescriptions and (ii) the recovery of W′ to aid human performance optimization.},
	number = {1},
	urldate = {2022-02-19},
	journal = {Sports Medicine - Open},
	author = {Sreedhara, Vijay Sarthy M. and Mocko, Gregory M. and Hutchison, Randolph E.},
	month = dec,
	year = {2019},
	keywords = {Critical power, Energy expenditure, Fatigue, Human performance modeling, Recovery, Time to exhaustion},
	pages = {54},
	file = {Full Text PDF:E\:\\Users\\John\\zotero\\storage\\LDRJI4C7\\Sreedhara 等。 - 2019 - A survey of mathematical models of human performan.pdf:application/pdf;Snapshot:E\:\\Users\\John\\zotero\\storage\\INED7CRV\\s40798-019-0230-z.html:text/html},
}

@article{oueslati_respiratory_2018,
	title = {Respiratory muscle strength is decreased after maximal incremental exercise in trained runners and cyclists},
	volume = {248},
	issn = {1569-9048},
	url = {https://www.sciencedirect.com/science/article/pii/S1569904817301957},
	doi = {10.1016/j.resp.2017.11.005},
	abstract = {The respiratory muscle fatigue seems to be able to limit exercise performance and may influence the determination of maximal oxygen uptake (V̇O2max) or maximum aerobic work rate during maximal incremental test. The aim of this study was therefore to investigate whether maximal incremental exercise decreases respiratory muscle strength. We hypothesized that respiratory muscle strength (maximal pressure) will decrease after maximal incremental exercise to exhaustion. 36 runners and 23 cyclists completed a maximal incremental test on a treadmill or a cycle ergometer with continuous monitoring of expired gases. Maximal inspiratory (MIP) and expiratory (MEP) pressure measurements were taken at rest and post- exercise. At rest, the MIP and MEP were 140±25 and 172±27 in runners vs. 115±26 and 146±33 in cyclists (p{\textless}0.05 between groups, respectively). The rest values of MIP and MEP were correlated to the V̇O2peak in all athletes, r=0.34, p{\textless}0.01 and r=0.36, p{\textless}0.01, respectively. At exhaustion, the MIP and MEP decreased significantly post- test by 13±7\% and 13±5\% in runners vs. 17±11\% and 15±10\% in cyclists (p{\textgreater}0.05), respectively. Our results suggest that respiratory muscle strength is decreased following maximal incremental exercise in trained runners and cyclists.},
	language = {en},
	urldate = {2022-02-19},
	journal = {Respiratory Physiology \& Neurobiology},
	author = {Oueslati, Ferid and Berriri, Ahmed and Boone, Jan and Ahmaidi, Said},
	month = jan,
	year = {2018},
	keywords = {MEP, MIP, Respiratory muscle fatigue},
	pages = {25--30},
}

@article{clark_comparative_2021,
	title = {A comparative analysis of critical power models in elite road cyclists},
	volume = {4},
	issn = {2665-9441},
	url = {https://www.sciencedirect.com/science/article/pii/S2665944121000171},
	doi = {10.1016/j.crphys.2021.05.001},
	abstract = {The aims of this study were to compare four different critical power model's ability to ascertain critical power and W′ in elite road cyclists, while making comparison to power output at respiratory compensation point, work rate (J·sec−1) at Wmax, and the work done above critical power during the Wmax test in relation to the W’. Ten male, elite endurance cyclists (V̇O2max ​= ​71.9 ​± ​5.9 ​ml ​kg−1·min−1) all familiar with critical power testing, participated in 3 testing sessions comprising 1. 15-s isokinetic (130 ​rpm) sprint, 1-min time trial, a ramp test to exhaustion, 2–3. a 4-min and/or 10-min self-paced maximal time trial separated by at least 24-h but limited to a 3-week period. The main findings show that all critical power models provided different W’ (F(1.061,8.486) ​= ​39.07, p ​= ​0.0002) and critical powers (F(1.022,8.179) ​= ​32.31, p ​= ​0.0004), while there was no difference between each model's critical power and power output at respiratory compensation point (F(1.155, 9.243) ​= ​2.72, p ​= ​0.131). Differences between models or comparisons with respiratory compensation point were deemed not clinically useful in the provision of training prescription or performance monitoring if the aim is to equal work rate at compensation point. There was also no post-hoc difference between work completed at Wmax (kJ) (p ​= ​0.890) and W′ using the nonlinear-3 model. Further research is required to investigate the physiological markers of intensity associated with respiratory compensation point and critical power work rate and the bioenergetic contribution to W’.},
	language = {en},
	urldate = {2022-02-19},
	journal = {Current Research in Physiology},
	author = {Clark, Boris and Macdermid, Paul W.},
	month = jan,
	year = {2021},
	keywords = {Critical power, Cycling, Training thresholds, W-prime, W’},
	pages = {139--144},
	file = {全文:E\:\\Users\\John\\zotero\\storage\\NM6HUGG9\\Clark 和 Macdermid - 2021 - A comparative analysis of critical power models in.pdf:application/pdf},
}

@article{fortes_effect_2020,
	title = {Effect of during of tapering on anaerobic power and capacity in road cyclists},
	volume = {35},
	issn = {0765-1597},
	url = {https://www.sciencedirect.com/science/article/pii/S0765159719302059},
	doi = {10.1016/j.scispo.2019.10.009},
	abstract = {The linear tapering method duration can be determinant for sport performance. Objective
The objective of this study was to analyze the effect of linear tapering duration method on anaerobic power and capacity in road cyclists.
Materials and methods
Seventeen male road cyclists, aged between 18 and 30 years were randomly selected within the study criteria. Participants performed 16 weeks of training, adopting the undulating periodization with weekly variation of the training load. The tapering phase lasted 4 weeks, using the linear tapering method, reducing only the training volume: 85\% in the first, 70\% in the second, 55\% in the third and 40\% in the fourth week. The Wingate test was used to evaluate anaerobic power and capacity. Wingate was performed by the cyclists before the start of the season, at the end of the last week of each mesocycle (Preparatory I, Specific I and Specific II) and at the end of each week in the tapering phase.
Results
The results revealed a time effect for anaerobic power (P{\textless}0.01) and capacity (P{\textless}0.01), with an increase after the preparatory phase I compared to pre-experimental (P=0.01), maintenance until phase specific II (P=0.01), maintenance in the third week of tapering (P{\textgreater}0.05), finishing with attenuation in the fourth week of tapering (P=0.01).
Conclusion
It was concluded that 2 weeks of tapering was enough to improve the anaerobic power and capacity in road cyclists.
Résumé
La réduction linéaire de la charge d’entraînement peut être déterminante pour la performance sportive.
Objectif
L’objectif de cette étude était d’analyser l’effet de la durée d’un affûtage linéaire sur la puissance anaérobie et la capacité dans la pratique cyclosportive.
Matériels et méthodes
Dix-sept cyclistes masculins, âgés entre 18 et 30 ans ont été choisis au hasard. Les participants ont effectué 16 semaines de formation, adoptant la périodisation ondulante avec la variation de la charge d’entraînement hebdomadaire. La phase d’affûtage a duré 4 semaines, en utilisant la méthode linéaire d’affûtage, réduisant uniquement le volume de formation : 85 \% dans la première, 70 \% dans la deuxième, 55 \% à la troisième et 40 \% dans la quatrième semaine. Le test de Wingate a été utilisé pour évaluer la capacité et puissance anaérobie. Wingate a été interprété par les cyclistes avant le début de la saison, à la fin de la dernière semaine de chaque mésocycle (préparatoire I, propres I et II spécifique) et à la fin de chaque semaine dans la phase d’affûtage.
Résultats
Les résultats révèlent un effet temps puissance anaérobie (p{\textless}0,01) et capacité (p{\textless}0,01), avec une augmentation après la phase préparatoire I, comparé au préalable expérimental (p=0,01), entretien jusqu’en phase II spécifique (p=0,01), entretien dans la troisième semaine d’affûtage (p{\textgreater}0,05), finissant par une atténuation dans la quatrième semaine d’affûtage (p=0,01).
Conclusion
Il a été conclu que deux semaines d’affûtage ont été suffisantes pour améliorer la puissance anaérobie et la capacité dans la pratique cyclosportive.},
	language = {en},
	number = {6},
	urldate = {2022-02-19},
	journal = {Science \& Sports},
	author = {Fortes, L. S. and Costa, B. D. V. and Paes, S. T. and Perez, A. J. and Diefenthaeler, F.},
	month = dec,
	year = {2020},
	keywords = {Cycling, Athlètes, Athletes, Cyclisme, Sport},
	pages = {330--335},
}

