	month = jan,
	year = {2016},
	keywords = {cycling, drag area, human power output, modeling, Tour de France},
	pages = {607--612},
	file = {ScienceDirect Full Text PDF:E\:\\Users\\John\\zotero\\storage\\HMG95AHI\\Hobson 和 Goff - 2016 - Tour de France Modeling 2015 Results and Comparis.pdf:application/pdf},
}

@article{vandewalle_effects_2019,
	title = {Effects of {Submaximal} {Performances} on {Critical} {Speed} and {Power}: {Uses} of an {Arbitrary}-{Unit} {Method} with {Different} {Protocols}},
	volume = {7},
	issn = {2075-4663},
	shorttitle = {Effects of {Submaximal} {Performances} on {Critical} {Speed} and {Power}},
	doi = {10.3390/sports7060136},
	abstract = {The effects of submaximal performances on critical speed (SCrit) and critical power (PCrit) were studied in 3 protocols: a constant-speed protocol (protocol 1), a constant-time protocol (protocol 2) and a constant-distance protocol (protocol 3). The effects of submaximal performances on SCrit and PCrit were studied with the results of two theoretical maximal exercises multiplied by coefficients lower or equal to 1 (from 0.8 to 1 for protocol 1; from 0.95 to 1 for protocols 2 and 3): coefficient C1 for the shortest exercises and C2 for the longest exercises. Arbitrary units were used for exhaustion times (tlim), speeds (or power-output in cycling) and distances (or work in cycling). The submaximal-performance effects on SCrit and PCrit were computed from two ranges of tlim (1-4 and 1-7). These effects have been compared for a low-endurance athlete (exponent = 0.8 in the power-law model of Kennelly) and a high-endurance athlete (exponent = 0.95). Unexpectedly, the effects of submaximal performances on SCrit and PCrit are lower in protocol 1. For the 3 protocols, the effects of submaximal performances on SCrit, and PCrit, are low in many cases and are lower when the range of tlim is longer. The results of the present theoretical study confirm the possibility of the computation of SCrit and PCrit from several submaximal exercises performed in the same session.},
	language = {eng},
	number = {6},
	journal = {Sports (Basel, Switzerland)},
	author = {Vandewalle, Henry},
	month = may,
	year = {2019},
	pmid = {31159341},
	pmcid = {PMC6628399},
	keywords = {critical power, critical speed, modelling, performance reliability},
	pages = {E136},
	file = {全文:E\:\\Users\\John\\zotero\\storage\\BRGIJ6B3\\Vandewalle - 2019 - Effects of Submaximal Performances on Critical Spe.pdf:application/pdf},
}

